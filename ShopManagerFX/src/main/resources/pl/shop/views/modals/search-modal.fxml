<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.shop.controller.modals.SearchModalController">

   <children>
      <VBox alignment="CENTER" spacing="10" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="20" AnchorPane.rightAnchor="20">
            <Label text="Wpisz czego szukasz" />
            <TextField fx:id="searchField" promptText="Wpisz..." />
            <Button mnemonicParsing="false" onAction="#searchActionButton" text="Szukaj" />
      </VBox>
   </children>

</AnchorPane>
